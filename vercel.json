{
  "version": 2,
  "buildCommand": "prisma generate",
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/redirect.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/healthz",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/code/(.*)",
      "dest": "/public/stats.html"
    },
    {
      "src": "/(css|js)/(.*)",
      "dest": "/public/$1/$2"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/([A-Za-z0-9]{6,8})",
      "dest": "/api/redirect.js"
    }
  ]
}
